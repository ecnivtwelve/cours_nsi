    "class TableMultiplication:\n",
    "  def __init__(self, ent):\n",
    "    self.ent = ent\n",
    "    self.current = -1\n",
    "  def prochain(self):\n",
    "    self.current += 1\n",
    "    return self.ent * self.current\n",
    "\n",
    "cinq = TableMultiplication(5)\n",
    "print(cinq.prochain())\n",
    "print(cinq.prochain())\n",
    "print(cinq.prochain())"
   ]
  }
